    "globals": "^15.10.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
